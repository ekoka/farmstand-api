POST /api/v1/clients
    => post_client(data) 

GET  /api/v1/clients/{client_name}
    => get_client(client_name)

POST /api/v1/clients/{client_name}/signins
    => post_signin(client_name, data)

POST /api/v1/clients/{client_name}/signin/password
    => post_signin_password(client_name, params, data)

GET /api/v1/clients/{client_name}/signin
    => get_signin(client_name, params)

POST /api/v1/clients/{client_name}/signin/access-token
    => post_signin_access_token(client_name, params)

GET /api/v1/clients/{client_name}/signin/access-token 
    => get_signin_access_token(client_name, params)

POST /api/v1/clients/{client_name}/signin/temporary-access-token
    => post_signin_temporary_access_token(client_name, data)
